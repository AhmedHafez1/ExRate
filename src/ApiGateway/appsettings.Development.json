{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "currencies": {
        "ClusterId": "currencies",
        "Match": {
          "Path": "currencies/{**catch-all}",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          {
            "PathPattern": "api/currencies/{**catch-all}"
          }
        ]
      }
    },
    "Serilog": {
      "MinimumLevel": {
        "Default": "Debug",
        "Override": {
          "Microsoft": "Warning",
          "System": "Warning"
        }
      },
      "WriteTo": [
        { "Name": "Console" }
      ],
      "Enrich": [ "FromLogContext" ]
    },
    "Clusters": {
      "currencies": {
        "Destinations": {
          "auctionApi": {
            "Address": "http://localhost:5001/"
          }
        }
      }
    }
  }
}
